{
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@jest/globals": "^29.7.0",
    "@munierujp/eslint-config-typescript": "^47.1.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@tsconfig/node16": "^16.1.3",
    "@tsconfig/node22": "^22.0.0",
