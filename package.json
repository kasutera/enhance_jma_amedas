{
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tsconfig/node24": "^24.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "glob": "^11.0.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lefthook": "^1.12.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.47.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "userscript-metadata": "^1.1.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs",
    "clean": "rimraf dist",
    "predev": "npm run clean",
    "dev": "rollup --config rollup.config.ts --configPlugin typescript --bundleConfigAsCjs --watch",
    "format": "npx @biomejs/biome check --write",
    "test": "jest --passWithNoTests",
    "release": "git tag $(date '+%Y%m%d') && git push --tags"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  }
}
